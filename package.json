{
  "name": "jmc1",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "set \"NODE_OPTIONS=--max-old-space-size=4096\" && set \"NEXT_PUBLIC_SERVER_URL=https://jmc111.vercel.app\" && next build",
    "build:local": "set \"NODE_OPTIONS=--max-old-space-size=4096\" && next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "analyze:deps": "npx depcheck",
    "analyze:size": "npx bundle-size-analyzer .next/static/chunks/*.js",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "next start"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@payloadcms/db-mongodb": "^3.41.0",
    "@payloadcms/live-preview-react": "3.41.0",
    "@payloadcms/next": "^3.41.0",
    "@payloadcms/payload-cloud": "3.41.0",
    "@payloadcms/plugin-form-builder": "^3.41.0",
    "@payloadcms/plugin-nested-docs": "^3.41.0",
    "@payloadcms/plugin-redirects": "^3.41.0",
    "@payloadcms/plugin-search": "^3.41.0",
    "@payloadcms/plugin-seo": "^3.41.0",
    "@payloadcms/richtext-lexical": "^3.41.0",
    "@payloadcms/storage-vercel-blob": "^3.41.0",
    "@payloadcms/ui": "^3.41.0",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^5.90.2",
    "@vercel/blob": "0.15.1",
    "aws4": "1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.5.0",
    "express-rate-limit": "7.5.0",
    "geist": "^1.3.1",
    "graphql": "^16.8.1",
    "lucide-react": "^0.468.0",
    "mongodb": "5.9.2",
    "next": "15.3.8",
    "next-sitemap": "4.2.3",
    "node-fetch": "3.3.2",
    "payload": "^3.41.0",
    "prism-react-renderer": "2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "14.3.8",
    "react-hook-form": "^7.54.0",
    "react-image-crop": "11.0.10",
    "sharp": "^0.33.5",
    "swiper": "^11.1.15",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.25.56"
  },
  "devDependencies": {
    "@eslint/eslintrc": "2.1.4",
    "@next/bundle-analyzer": "15.3.3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@tailwindcss/typography": "0.5.16",
    "@types/express": "5.0.3",
    "@types/node": "20.10.4",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "autoprefixer": "10.4.21",
    "depcheck": "1.4.7",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "postcss": "8.5.4",
    "prettier": "3.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.3.3",
    "webpack-bundle-analyzer": "4.10.2"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "react": "19.0.0",
      "react-dom": "19.0.0"
    }
  }
}
